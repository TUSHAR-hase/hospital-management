
{
    "rewrites": [{ 
        "source": "/api/(.*)", 
        "destination": "/api" 
    }],
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     

     }
    ]
   }